{
   "manifest_version": 2,

   "name": "Friend Safari Notifier",
   "description": "Get notified when a friend safari of interest is posted to r/friendsafari",
   "version": "1.0",

   "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png" },

   "permissions": [
      "http://www.reddit.com/r/friendsafari/",
      "alarms"
      ],

   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   }
}
